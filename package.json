{
  "name": "diyws",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "docker:dev:build": "docker build -t diyws-dev -f Dockerfile.dev .",
    "docker:dev:init-container": "docker run --name diyws-dev -p 3000:3000 -v /app/node_modules -v .:/app diyws-dev",
    "docker:dev:start": "docker start -a diyws-dev",
    "docker:prod:build": "docker build -t diyws -f Dockerfile.prod .",
    "docker:prod:init-container": "docker run --name diyws -p 3000:3000 diyws",
    "docker:prod:start": "docker start -a diyws"
  },
  "dependencies": {
    "@renatodellosso/typed-api-client": "^1.0.9",
    "@types/dockerode": "^3.3.44",
    "cpu-features": "^0.0.10",
    "dockerode": "^4.0.9",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.15",
    "@types/node": "^20.19.22",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "daisyui": "^5.3.7",
    "tailwindcss": "^4.1.15",
    "typescript": "^5.9.3"
  }
}
