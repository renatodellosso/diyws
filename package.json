{
  "name": "diyws",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "node scripts/setup.ts",
    "dev": "docker compose -f compose.yaml.dev up",
    "start": "docker compose -f compose.yaml.prod up",
    "build": "docker build -t diyws-dev -f Dockerfile.dev . & docker build -t diyws -f Dockerfile.prod . & wait",
    "internal:dev": "NODE_ENV=development node index.ts",
    "internal:build": "next build --turbopack",
    "internal:start": "NODE_ENV=production node index.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@renatodellosso/typed-api-client": "^1.0.12",
    "@types/dockerode": "^3.3.45",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "cpu-features": "^0.0.10",
    "dockerode": "^4.0.9",
    "next": "15.5.6",
    "next-auth": "^4.24.11",
    "os": "^0.1.2",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^20.19.23",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "daisyui": "^5.3.10",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3"
  }
}
